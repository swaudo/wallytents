<header>
<header class="navbar navbar-fixed-top"">
  <div class="navbar-inner">
    <div class="container">
      <a class="btn btn-navbar" data-target=".nav-collapse" data-toggle="collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
      <%= link_to "Wally Tents", root_path, id: "logo" %>
      <nav class="container nav-collapse">
        <ul class="nav">
          <li><%= link_to '<i class="icon-home icon-white"></i> home'.html_safe, root_path %></li>
          <li class="divider-vertical"></li>
          <li><%= link_to '<i class="icon-question-sign icon-white"></i> about us'.html_safe, about_path %></li>
          <li class="divider-vertical"></li>
          <li><%= link_to '<i class="icon-home icon-white"></i> pricing'.html_safe, root_path %></li>
          <li class="divider-vertical"></li>
          <li><%= link_to '<i class="icon-question-sign icon-white"></i> services'.html_safe, help_path %></li>
          <li class="divider-vertical"></li>
          <li><%= link_to '<i class="icon-home icon-white"></i> contact'.html_safe, root_path %></li>
          <li class="divider-vertical"></li>
          
          <% if signed_in? %>
            <li><%= link_to '<i class="icon-group icon-white"></i> users'.html_safe, users_path %></li>
            <li class="divider-vertical"></li>
            <li id="fat-menu" class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Account <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><%= link_to '<i class="icon-user icon-white"></i> profile'.html_safe, current_user %></li>
                <li><%= link_to '<i class="icon-cog icon-white"></i> settings'.html_safe, edit_user_path(current_user) %></li>
                
                <li>
                  <%= link_to "Sign out", signout_path, method: "delete" %>
                </li>
                
              </ul>
            </li>
          <% else %>
          </ul>
          <ul class="nav pull-right">
            <li><%= link_to "Sign in", signin_path %></li>
          </ul>
          <% end %>
        
      </nav>
    </div>
  </div>
</header>